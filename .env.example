# Copy this file to .env and fill in the secrets before running docker compose.
# Only the executor service needs an LLM key.
REMOTE_BROWSER_TOOL_LLM__PROVIDER=openai
REMOTE_BROWSER_TOOL_LLM__MODEL=gpt-4o-mini
REMOTE_BROWSER_TOOL_LLM__API_KEY=sk-your-openai-key
# Uncomment to override additional defaults for every run.
# REMOTE_BROWSER_TOOL_BROWSER__HEADLESS=false
# REMOTE_BROWSER_TOOL_BROWSER__ENABLE_VNC=true
# REMOTE_BROWSER_TOOL_PORTAL__PORT=8765
